

x = 0.1:1/22:1; %Iejimas

is = = sin(2*pi*x)/2; 

% y = (1 + 0.6*sin(2*pi*x/0.7)) + 0.3*sin(2*pi*x))/2;

w1 = rand(1);
w2 = rand(1);
b = randn(1);
n = 0.01;

%Centru reiksmes c1 ir c2, spinduliu reiksmes r1 ir r2
c1 = ;
c2 = ;
r1 = ;
r2 = ;

for i = 1:100
    for k = 1:20
      y = gauss(x(n),r1,c1)*w1+gauss(x(n),r2,c2)*w2+b;
      e = is(n) - y;
      b = b+n*e;
      w1 = w1+n*e*gauss(x(n),r1,c1);
      w2 = w2+n*e*gauss(x(n),r2,c2);
    end
    
    e = e-1;
end
    
